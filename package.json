{
	"name": "crm-vue-supabase",
	"version": "1.0.0",
	"description": "CRM System based on Vue 3 with Supabase SDK",
	"author": "igorexa34314",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/igorexa34314/crm-vue-supabase.git"
	},
	"scripts": {
		"dev": "vite",
		"build": "run-p type-check \"build-only {@}\" --",
		"preview": "vite preview",
		"build-only": "vite build",
		"build:analyze": "run-s type-check \"build --mode=analyze\"",
		"type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
		"format": "prettier --write --cache src/",
		"db:types": "supabase gen types typescript --project-id fgnzylfktyuvrswdvgcv > database.types.ts"
	},
	"dependencies": {
		"@mdi/js": "^7.4.47",
		"@supabase/supabase-js": "^2.39.3",
		"@unhead/vue": "^1.8.10",
		"@vueuse/core": "^10.7.2",
		"chart.js": "^4.4.1",
		"lodash": "^4.17.21",
		"pinia": "^2.1.7",
		"randomcolor": "^0.6.2",
		"uuid": "^9.0.1",
		"vite-plugin-vuetify": "^2.0.1",
		"vue": "^3.4.15",
		"vue-chartjs": "^5.3.0",
		"vue-i18n": "^9.9.0",
		"vue-router": "^4.2.5",
		"vuetify": "^3.5.1",
		"vuetify-birthdaypicker": "^0.0.3"
	},
	"devDependencies": {
		"@intlify/unplugin-vue-i18n": "^2.0.0",
		"@rushstack/eslint-patch": "^1.7.0",
		"@tsconfig/node18": "^18.2.2",
		"@types/lodash": "^4.14.202",
		"@types/node": "^20.11.5",
		"@types/randomcolor": "^0.5.9",
		"@types/uuid": "^9.0.7",
		"@vitejs/plugin-vue": "^5.0.3",
		"@vue/eslint-config-prettier": "^9.0.0",
		"@vue/eslint-config-typescript": "^12.0.0",
		"@vue/tsconfig": "^0.5.1",
		"eslint": "^8.56.0",
		"eslint-plugin-vue": "^9.20.1",
		"npm-run-all2": "^6.1.1",
		"prettier": "^3.2.4",
		"rollup-plugin-visualizer": "^5.12.0",
		"sass": "^1.70.0",
		"supabase": "^1.131.5",
		"turnstile-types": "^1.2.0",
		"typescript": "^5.3.3",
		"unplugin-vue-router": "^0.7.0",
		"vite": "^5.0.12",
		"vite-plugin-vue-layouts": "^0.11.0",
		"vue-tsc": "^1.8.27"
	},
	"packageManager": "pnpm@8.7.0"
}
