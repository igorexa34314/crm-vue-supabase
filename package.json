{
	"name": "crm-vue-supabase",
	"version": "2.0.0",
	"description": "CRM System based on Vue 3 with Supabase SDK",
	"author": "igorexa34314",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/igorexa34314/crm-vue-supabase.git"
	},
	"scripts": {
		"dev": "vite",
		"build": "run-p type-check \"build-only {@}\" --",
		"preview": "vite preview",
		"build-only": "vite build",
		"analyze": "run-s type-check \"build --mode=analyze\"",
		"type-check": "vue-tsc --build --force",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
		"format": "prettier --write --cache src/",
		"generate-types": "supabase gen types typescript --project-id fgnzylfktyuvrswdvgcv > ./src/types/database-generated.types.ts"
	},
	"dependencies": {
		"@mdi/js": "^7.4.47",
		"@supabase/supabase-js": "^2.45.0",
		"@unhead/vue": "^1.9.16",
		"@vueuse/core": "^10.11.0",
		"@vueuse/router": "^10.11.0",
		"chart.js": "^4.4.3",
		"deep-equal": "^2.2.3",
		"pinia": "^2.2.0",
		"randomcolor": "^0.6.2",
		"uuid": "^10.0.0",
		"vue": "^3.4.34",
		"vue-chartjs": "^5.3.1",
		"vue-i18n": "^9.13.1",
		"vue-router": "^4.4.0",
		"vuetify": "^3.6.14",
		"vuetify-birthdaypicker": "^0.0.3"
	},
	"devDependencies": {
		"@intlify/unplugin-vue-i18n": "^4.0.0",
		"@rushstack/eslint-patch": "^1.10.4",
		"@tsconfig/node20": "^20.1.4",
		"@types/cloudflare-turnstile": "^0.2.0",
		"@types/deep-equal": "^1.0.4",
		"@types/node": "^22.1.0",
		"@types/randomcolor": "^0.5.9",
		"@types/uuid": "^10.0.0",
		"@vitejs/plugin-vue": "^5.1.2",
		"@vue/eslint-config-prettier": "^9.0.0",
		"@vue/eslint-config-typescript": "^13.0.0",
		"@vue/tsconfig": "^0.5.1",
		"eslint": "^8.57.0",
		"eslint-plugin-vue": "^9.27.0",
		"npm-run-all2": "^6.2.2",
		"prettier": "^3.3.3",
		"rollup-plugin-visualizer": "^5.12.0",
		"sass": "^1.77.6",
		"supabase": "^1.187.10",
		"type-fest": "^4.23.0",
		"typescript": "^5.5.4",
		"unplugin-vue-router": "^0.10.2",
		"vite": "^5.3.5",
		"vite-plugin-vue-devtools": "^7.3.7",
		"vite-plugin-vue-layouts": "^0.11.0",
		"vite-plugin-vuetify": "^2.0.4",
		"vue-tsc": "^2.0.29"
	},
	"packageManager": "pnpm@9.6.0"
}
