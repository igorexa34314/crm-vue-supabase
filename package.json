{
	"name": "crm-vue-supabase",
	"version": "1.0.0",
	"description": "CRM System based on Vue 3 with Supabase SDK",
	"author": "igorexa34314",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/igorexa34314/crm-vue-supabase.git"
	},
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc && vite build && cp dist/index.html dist/404.html",
		"build:analyze": "export ROLLUP_ANALYZE=true || set ROLLUP_ANALYZE=true && vue-tsc && vite build",
		"preview": "vite preview",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
		"format": "prettier --write --cache src/",
		"db:types": "supabase gen types typescript --project-id fgnzylfktyuvrswdvgcv > database.types.ts"
	},
	"dependencies": {
		"@mdi/js": "^7.3.67",
		"@supabase/supabase-js": "^2.38.1",
		"@vueuse/core": "^10.5.0",
		"chart.js": "^4.4.0",
		"lodash": "^4.17.21",
		"pinia": "^2.1.7",
		"randomcolor": "^0.6.2",
		"uuid": "^9.0.1",
		"vite-plugin-vuetify": "^1.0.2",
		"vue": "^3.3.4",
		"vue-chartjs": "^5.2.0",
		"vue-i18n": "^9.5.0",
		"vue-meta": "3.0.0-alpha.10",
		"vue-router": "^4.2.5",
		"vuetify": "^3.3.21"
	},
	"devDependencies": {
		"@intlify/unplugin-vue-i18n": "^1.4.0",
		"@rushstack/eslint-patch": "^1.5.1",
		"@types/lodash": "^4.14.199",
		"@types/node": "^20.8.6",
		"@types/randomcolor": "^0.5.7",
		"@types/uuid": "^9.0.6",
		"@vitejs/plugin-vue": "^4.4.0",
		"@vue/eslint-config-prettier": "^8.0.0",
		"@vue/eslint-config-typescript": "^12.0.0",
		"@vue/tsconfig": "^0.4.0",
		"eslint": "^8.51.0",
		"eslint-plugin-vue": "^9.17.0",
		"prettier": "^3.0.3",
		"rollup-plugin-visualizer": "^5.9.2",
		"sass": "^1.69.3",
		"supabase": "^1.106.1",
		"turnstile-types": "^1.1.3",
		"typescript": "^5.2.2",
		"unplugin-vue-router": "^0.7.0",
		"vite": "^4.4.11",
		"vite-plugin-vue-layouts": "^0.8.0",
		"vue-tsc": "^1.8.19"
	},
	"packageManager": "pnpm@8.7.0"
}