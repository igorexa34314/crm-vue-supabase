		"@vue/eslint-config-typescript": "^11.0.3",
		"@vue/tsconfig": "^0.4.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.48.0",
		"eslint-plugin-vue": "^9.17.0",
		"prettier": "^3.0.2",
		"rollup-plugin-visualizer": "^5.9.2",
		"sass": "^1.66.1",
		"supabase": "^1.88.0",
		"typescript": "^5.2.2",
		"unplugin-vue-router": "^0.6.4",
		"vite": "^4.4.9",
