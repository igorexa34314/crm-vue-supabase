{
	"name": "crm-vue-supabase",
	"version": "1.0.0",
	"description": "CRM System based on Vue 3 with Supabase SDK",
	"author": "igorexa34314",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/igorexa34314/crm-vue-supabase.git"
	},
	"scripts": {
		"dev": "vite",
		"build": "run-p type-check \"build-only {@}\" --",
		"preview": "vite preview",
		"build-only": "vite build",
		"build:analyze": "run-s type-check \"build --mode=analyze\"",
		"type-check": "vue-tsc --build --force",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
		"format": "prettier --write --cache src/",
		"db:types": "supabase gen types typescript --project-id fgnzylfktyuvrswdvgcv > ./src/types/types/database.types.ts"
	},
	"dependencies": {
		"@mdi/js": "^7.4.47",
		"@supabase/supabase-js": "^2.40.0",
		"@unhead/vue": "^1.9.1",
		"@vueuse/core": "^10.9.0",
		"chart.js": "^4.4.2",
		"cross-fetch": "^4.0.0",
		"lodash": "^4.17.21",
		"pinia": "^2.1.7",
		"randomcolor": "^0.6.2",
		"uuid": "^9.0.1",
		"vue": "^3.4.21",
		"vue-chartjs": "^5.3.0",
		"vue-i18n": "^9.10.2",
		"vue-router": "^4.3.0",
		"vuetify": "^3.5.11",
		"vuetify-birthdaypicker": "^0.0.3"
	},
	"devDependencies": {
		"@intlify/unplugin-vue-i18n": "^2.0.0",
		"@rushstack/eslint-patch": "^1.8.0",
		"@tsconfig/node20": "^20.1.3",
		"@types/lodash": "^4.17.0",
		"@types/node": "^20.11.30",
		"@types/randomcolor": "^0.5.9",
		"@types/uuid": "^9.0.8",
		"@vitejs/plugin-vue": "^5.0.4",
		"@vue/eslint-config-prettier": "^9.0.0",
		"@vue/eslint-config-typescript": "^12.0.0",
		"@vue/tsconfig": "^0.5.1",
		"eslint": "^8.57.0",
		"eslint-plugin-vue": "^9.24.0",
		"npm-run-all2": "^6.1.2",
		"prettier": "^3.2.5",
		"rollup-plugin-visualizer": "^5.12.0",
		"sass": "^1.72.0",
		"supabase": "^1.150.0",
		"turnstile-types": "^1.2.0",
		"typescript": "^5.4.3",
		"unplugin-vue-router": "^0.7.0",
		"vite": "^5.2.6",
		"vite-plugin-vue-devtools": "^7.0.20",
		"vite-plugin-vue-layouts": "^0.11.0",
		"vite-plugin-vuetify": "^2.0.3",
		"vue-tsc": "^2.0.7"
	},
	"packageManager": "pnpm@8.15.5"
}
