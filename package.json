{
	"name": "crm-vue-supabase",
	"version": "2.2.0",
	"private": true,
	"description": "CRM System based on Vue 3 with Supabase SDK",
	"author": "igorexa34314",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/igorexa34314/crm-vue-supabase.git"
	},
	"scripts": {
		"dev": "vite",
		"build": "run-p typecheck \"build-only {@}\" --",
		"preview": "vite preview",
		"build-only": "vite build",
		"analyze": "pnpx vite-bundle-visualizer",
		"typecheck": "vue-tsc --build",
		"lint": "eslint . --cache",
		"lint:fix": "eslint . --fix --cache",
		"format": "prettier --write --cache src/",
		"generate:types": "pnpx supabase gen types typescript --project-id sxjleofipivdmluufxic > ./src/types/database-generated.ts"
	},
	"dependencies": {
		"@mdi/js": "^7.4.47",
		"@supabase/supabase-js": "^2.56.0",
		"@unhead/vue": "^2.0.14",
		"@vueuse/core": "^13.8.0",
		"@vueuse/router": "^13.8.0",
		"chart.js": "^4.5.0",
		"deep-equal": "^2.2.3",
		"klona": "^2.0.6",
		"pinia": "^3.0.3",
		"randomcolor": "^0.6.2",
		"ufo": "^1.6.1",
		"uuid": "^11.1.0",
		"vue": "^3.5.20",
		"vue-chartjs": "^5.3.2",
		"vue-i18n": "^11.1.11",
		"vue-router": "^4.5.1",
		"vuetify": "^3.9.6",
		"vuetify-birthdaypicker": "^0.0.3"
	},
	"devDependencies": {
		"@intlify/unplugin-vue-i18n": "^11.0.0",
		"@tsconfig/node20": "^20.1.6",
		"@types/cloudflare-turnstile": "^0.2.2",
		"@types/deep-equal": "^1.0.4",
		"@types/node": "^22.18.0",
		"@types/randomcolor": "^0.5.9",
		"@types/uuid": "^10.0.0",
		"@vitejs/plugin-vue": "^6.0.1",
		"@vue/eslint-config-prettier": "^10.2.0",
		"@vue/eslint-config-typescript": "^14.6.0",
		"@vue/tsconfig": "^0.8.1",
		"eslint": "^9.34.0",
		"eslint-plugin-vue": "^10.4.0",
		"jiti": "^2.5.1",
		"lint-staged": "^16.1.5",
		"npm-run-all2": "^8.0.4",
		"prettier": "^3.6.2",
		"sass-embedded": "^1.91.0",
		"simple-git-hooks": "^2.13.1",
		"type-fest": "^5.0.0",
		"typescript": "^5.9.2",
		"unplugin-vue-router": "^0.15.0",
		"vite": "^7.1.3",
		"vite-plugin-vue-devtools": "^8.0.1",
		"vite-plugin-vuetify": "^2.1.2",
		"vue-tsc": "^3.0.6"
	},
	"packageManager": "pnpm@10.15.1",
	"pnpm": {
		"onlyBuiltDependencies": [
			"@parcel/watcher",
			"esbuild",
			"simple-git-hooks"
		]
	},
	"simple-git-hooks": {
		"pre-commit": "pnpm lint-staged"
	},
	"lint-staged": {
		"*": "eslint --fix"
	}
}
