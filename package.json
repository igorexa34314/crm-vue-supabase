{
	"name": "crm-vue-supabase",
	"version": "1.0.0",
	"description": "CRM System based on Vue 3 with Supabase SDK",
	"author": "igorexa34314",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/igorexa34314/crm-vue-supabase.git"
	},
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc && vite build && cp dist/index.html dist/404.html",
		"build:analyze": "export ROLLUP_ANALYZE=true || set ROLLUP_ANALYZE=true && vue-tsc && vite build",
		"preview": "vite preview",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
		"format": "prettier --write --cache src/"
	},
	"dependencies": {
		"@mdi/js": "^7.2.96",
		"@supabase/supabase-js": "^2.33.1",
		"@vueuse/core": "^10.4.1",
		"chart.js": "^4.4.0",
		"lodash": "^4.17.21",
		"pinia": "^2.1.6",
		"randomcolor": "^0.6.2",
		"uuid": "^9.0.0",
		"vite-plugin-vuetify": "^1.0.2",
		"vue": "^3.3.4",
		"vue-chartjs": "^5.2.0",
		"vue-i18n": "^9.2.2",
		"vue-meta": "3.0.0-alpha.10",
		"vue-router": "^4.2.4",
		"vuetify": "^3.3.14"
	},
	"devDependencies": {
		"@intlify/unplugin-vue-i18n": "^0.12.3",
		"@rushstack/eslint-patch": "^1.3.3",
		"@types/lodash": "^4.14.197",
